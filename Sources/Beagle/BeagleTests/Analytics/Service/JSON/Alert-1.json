{
    "_beagleAction_": "beagle:alert",
    "analytics": {
        "attributes": [
            "labelOk",
            "message",
            "title"
        ],
        "additionalEntries": {
            "labelOk": "additionalEntries takes precedence"
        }
    },
    "labelOk": "ok",
